<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.aulashelder.itents" >

    <uses-sdk
        android:minSdkVersion="5"
        android:targetSdkVersion="17" />

    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="false"
        android:icon="@drawable/ic_launcher"
        android:theme="@style/AppTheme" >
        <activity
            android:name="br.aulashelder.itents.Menu"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
   
        <activity
            android:name="br.aulashelder.itents.AbrirMapaEnderecoActivity"
            android:label="AbrirMapaEnderecoActivity" />
        <activity
            android:name="br.aulashelder.itents.ExemploAbrirBrowser"
            android:label="ExemploAbrirBrowser" />
        <activity
            android:name="br.aulashelder.itents.LigarParaTelefoneActivity"
            android:label="LigarParaTelefoneActivity" />
        <activity
            android:name="br.aulashelder.itents.VisualizarContatoId1"
            android:label="VisualizarContatoId1" />
        <activity
            android:name="br.aulashelder.itents.VisualizarTodosContatos"
            android:label="VisualizarTodosContatos" />
      
    </application>

</manifest>